	</head>
	{% if not grav.uri.param('onlydisplaypagecontent') %}
	<body class="{% block body_classes %}{{ page.header.body_classes }}{% endblock %}">
		{% block header_navigation %}
			{% include 'partials/navigation.html.twig' %}
			{% endblock %}
	{% endif %}
	{% block content %}{% endblock %}
	{% block pagination %}{% endblock %}
	{% if not grav.uri.param('onlydisplaypagecontent') %}
		<footer class="page-footer">
		<div class="footer-copyright">
			<div class="container">
					<div class="footer-nav">
						<ul>
							{% for page in pages.children.visible %}
								<li><a href="{{ page.url }}">{{ page.menu }}</a></li>
							{% endfor %}
							{% for mitem in site.menu %}
								<li>
									<a class="newwindow" href="{{ mitem.url }}" title="{{ mitem.title }}">
										{% if mitem.icon %}
											<i class="mdi mdi-{{ mitem.icon }}"></i>
											<span>{{ mitem.text }}</span>
										{% else %}
											{{ mitem.text }}
										{% endif %}
									</a>
								</li>
							{% endfor %}
							{% if config.site.github.enabled %}
								<li>
									{% include 'partials/github_text_link.html.twig' %}
								</li>
							{% endif %}
						</ul>
						<br>
					</div>
					<a rel="license" href="http://creativecommons.org/licenses/by-sa/4.0/"><img alt="Creative Commons Licence" style="border-width:0" src="https://i.creativecommons.org/l/by-sa/4.0/88x31.png" /></a><br />Except where otherwise noted, content on this site is licensed under a <a rel="license" href="http://creativecommons.org/licenses/by-sa/4.0/">Creative Commons Attribution-ShareAlike 4.0 International License</a>.
					<br><br>
					Materialize Theme for <a href="http://getgrav.org">Grav</a>
			</div>
		</div>
	</footer>
	{% endif %}
	{% block bottom %}
		<script>
			$(document).ready(function () {
				$(".button-collapse").sideNav({
						closeOnClick: true
					}
				);
			})
		</script>
	{% endblock %}
</body>
</html>
